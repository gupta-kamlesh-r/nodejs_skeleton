{
  "name": "nodejs-skeleton",
  "version": "1.0.0",
  "description": "Node.js api using express framework and mysql database and provide modular architecture\"",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server.js",
    "dev": "nodemon server.js",
    "doc": "apidoc -i server/ -o apidoc/",
    "sequelize": "node_modules/.bin/sequelize",
    "db-connection": "node server/database/test-connection.js --debug",
    "migrate": "sequelize db:migrate",
    "rollback": "sequelize db:migrate:undo",
    "seed": "sequelize db:seed:all --debug",
    "rollback-seed": "sequelize db:seed:undo:all",
    "generate-migration": "sequelize migration:generate --name",
    "generate-model": "sequelize model:generate --name",
    "generate-seed": "sequelize seed:generate --name"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gupta-kamlesh-r/nodejs_skeleton.git"
  },
  "keywords": [
    "NodeJs",
    "ExpressJS",
    "Module",
    "Architecture",
    "REST",
    "API",
    "Sequelize",
    "Mysql"
  ],
  "author": "Kamlesh Gupta",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gupta-kamlesh-r/nodejs_skeleton/issues"
  },
  "homepage": "https://github.com/gupta-kamlesh-r/nodejs_skeleton#readme",
  "dependencies": {
    "bcrypt": "^3.0.0",
    "body-parser": "^1.18.3",
    "colors": "^1.3.2",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "express-limiter": "^1.6.1",
    "express-validator": "^5.3.0",
    "helmet": "^3.13.0",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.10",
    "method-override": "^3.0.0",
    "moment": "^2.22.2",
    "mysql2": "^1.6.1",
    "nodemon": "^1.18.3",
    "sequelize": "^4.38.0",
    "sequelize-cli": "^4.1.1"
  },
  "devDependencies": {
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0"
  }
}
